<h1>Post you're art</h1>
<form
  method="POST"
  action="/art/create/<%= currentUser._id %>"
  enctype="multipart/form-data"
>
<nav id="Post">
  <div>
    <label>Title </label>
    <br />
    <input name="title" />
  </div>

  <div>
    <label>Description </label>
    <br />
    <textarea
      name="description"
      cols="30"
      rows="10"
      placeholder="Tell us about your Art"
    ></textarea>
  </div>

  <div>
    <label> Price </label>
    <br />
    <input type="number" name="price" />
  </div>

  <div>
    <label>Upload Art Image </label>
    <br />
    <input type="file" name="image" />
  </div>
<br>
<div>
  <button type="submit">Create Art</button>

</form>
</nav>