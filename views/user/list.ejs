<div class="container">
  <div class="artList__container">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <div class="bestSeller__tittle text-center">
            <h2>Artists We Work With</h2>
          </div>
        </div>
      </div>
      <div class="row">
        <%user.forEach(user => {%> <%if(user.userType == "artist"){%>
        <div class="col-lg-4 col-sm-6">
          <div class="single__item">
            <div>
              <a href="/user/show/<%= user._id %>"
                ><img src="<%= user.image %>" width="100%" height="300px"
              /></a>
            </div>
            <a href="/user/show/<%= user._id %>"
              ><h3><%= user.firstname %> <%= user.lastname %></h3></a
            >
            <p><%= user.email %></p>
            <p><%= user.phone %></p>
          </div>
        </div>
        <%}%> <%})%>
      </div>
    </div>
  </div>
</div>
