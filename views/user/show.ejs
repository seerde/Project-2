<!-- breadcrumb part start-->
<section class="breadcrumb_part">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="breadcrumb_iner">
          <h2><%= user.firstname %>'s Work</h2>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- breadcrumb part end-->
<!-- trending item start-->
<section class="trending_items">
  <div class="container">
    <div class="row">
      <%user.art.forEach(art => {%>
      <div class="col-lg-4 col-sm-6">
        <div class="single_product_item">
          <div class="single_product_item_thumb">
            <img src="<%= art.image %>" alt="#" width="400" height="300" />
          </div>
          <p><%= art.title %></p>
          <h3>
            <a href="single-product.html"><%= art.description %></a>
          </h3>
          <p>For $<%= art.price %></p>
        </div>
      </div>
      <%})%>
    </div>
  </div>
</section>
<!-- trending item end-->
